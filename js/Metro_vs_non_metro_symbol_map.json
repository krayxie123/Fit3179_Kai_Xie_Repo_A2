{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Victorian Schools by Sector",
  "width": 400,
  "height": 520,
  "projection": {"type": "mercator"},
  "layer": [
    {
      "data": {
        "url": "js/vicsuburbsgood.topojson",
        "format": {"type": "topojson", "feature": "vicsuburbsgood"}
      },
      "mark": {"type": "geoshape", "fill": "#f5f5f5", "stroke": "#ccc"}
    },
    {
      "data": {
        "url": "data/dv402-SchoolLocations2025.csv"
      },
      "mark": {"type": "circle", "size": 24, "opacity": 0.85},
      "encoding": {
        "longitude": {"field": "X", "type": "quantitative"},
        "latitude": {"field": "Y", "type": "quantitative"},
        "color": {"field": "LGA_TYPE", "type": "nominal"},
        "tooltip": [
          {"field": "School_Name", "type": "nominal", "title": "School"},
          {"field": "Address_Town", "type": "nominal", "title": "Town"}
        ]
      }
    }
  ]
}